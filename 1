local Items = {}
local function getItems()
    for i,v in pairs(game:GetService("ReplicatedStorage").Purchasables:GetDescendants()) do
        if v:FindFirstChild("Box") and v:FindFirstChild("Price") and v:FindFirstChild("AssetId") then
            table.insert(Items,tostring(v.ItemName.Value))
        end
    end
end
local function getItem(Name)
    for i,v in pairs(game:GetService("ReplicatedStorage").Purchasables:GetDescendants()) do
        if v:FindFirstChild("Box") and v:FindFirstChild("Price") and v.ItemName.Value == Name then
            return v
        end
    end
end
local SpawnClient = function(item)
    if item:FindFirstChild("Box") or item:FindFirstChild("Model"):FindFirstChild("DraggableItem") then


        local ITEM
        if item:FindFirstChild("Box") then
        ITEM = item.Box:Clone()
        else
        if item:FindFirstChild("Model") then
        ITEM = item.Model:Clone()
        end
        end
        ITEM.Parent = game.Workspace.PlayerModels
        local Owner = Instance.new("ObjectValue", ITEM)
        Owner.Value = game.Players.LocalPlayer
        Owner.Name = "Owner"
        local Owner2 = Instance.new("StringValue", Owner)
        Owner2.Value = game.Players.LocalPlayer.Name
        Owner2.Name = "OwnerString"
        local l = Instance.new("IntValue", Owner)
        l.Name = "LastInteraction"
        ITEM:MoveTo(game.Players.LocalPlayer.Character.Head.Position)
        if ITEM:FindFirstChild("BoxItemName") then
        else
            local boxItem = Instance.new("StringValue", ITEM)
            boxItem.Value = item.Name
        end
        ITEM.BoxItemName.Name = "PurchasedBoxItemName"
        --game.ServerStorage.SendLog:Fire(game.Players.LocalPlayer.Name .. " spawned an item called: " .. ITEM.PurchasedBoxItemName.Value)
    end
end
getItems()
