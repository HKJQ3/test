local Item
local function getShop(Item)
    for i,v in ipairs(workspace.Stores:GetDescendants()) do
        if v.Name == item then
            local sj = v.Parent.Parent
            for _,n in pairs(sj:GetDescendants()) do
                if n:IsA("Humanoid") then
                    shoper = n.Parent.Name
                end
            end
            Item = v
        end
    end
end
local Blueprints = {}
for i,v in ipairs(workspace.Stores:GetChildren()) do
    if v:FindFirstChild("ShopItems") then
        local ShopItems = v["ShopItems"]
        for o,b in ipairs(ShopItems:GetChildren()) do
            if not table.find(Blueprints,tostring(b.Name)) and b:FindFirstChild("Type") and b:FindFirstChild("Type").Value == "Blueprint" then
                table.insert(Blueprints,tostring(b.Name))
            end
        end
    end
end
local listen = function(items)
    a = workspace.PlayerModels.ChildAdded:Connect(function(child)
        local owner = child:WaitForChild("Owner")
        if owner.Value == lp and child.Name:find(items) then   
            if to then return end
                game:GetService("ReplicatedStorage").PlaceStructure.ClientPlacedStructure:FireServer(nil,lp.Character.HumanoidRootPart.CFrame,lp, nil, child, true)         
                local bypassanticheat = "Ifyouarereadingthisstophackingbrolegitalsokrnlisbadbtw432rewdWdwFe432432rwDWDAVWdawgdGWAYDFGYTUW"
                game:GetService("ReplicatedStorage").Interaction.ClientIsDragging:FireServer(child,bypassanticheat)
                child:MoveTo(lp.Character.HumanoidRootPart.CFrame.p)
                game:GetService("ReplicatedStorage").Interaction.ClientIsDragging:FireServer(child,bypassanticheat)
                game:GetService('RunService').Stepped:wait();
                game:GetService("ReplicatedStorage").PlaceStructure.ClientPlacedStructure:FireServer(nil,lp.Character.HumanoidRootPart.CFrame,lp, nil, child, true)                       
              a:Disconnect()
            return true
        end
        if owner.Value == lp and child.Name:find(items) and to then        
                game:GetService("ReplicatedStorage").PlaceStructure.ClientPlacedStructure:FireServer(nil,buyset.p,lp, nil, child, true)         
                local bypassanticheat = "Ifyouarereadingthisstophackingbrolegitalsokrnlisbadbtw432rewdWdwFe432432rwDWDAVWdawgdGWAYDFGYTUW"
                game:GetService("ReplicatedStorage").Interaction.ClientIsDragging:FireServer(child,bypassanticheat)
                child:MoveTo(buyset.p)
                game:GetService("ReplicatedStorage").Interaction.ClientIsDragging:FireServer(child,bypassanticheat)
                game:GetService('RunService').Stepped:wait();
                game:GetService("ReplicatedStorage").PlaceStructure.ClientPlacedStructure:FireServer(nil,buyset.p,lp, nil, child, true)                       
              a:Disconnect()
            return true
        end
    end)
end
local num
local function FindCharacter(Name)
    for i,v in ipairs(workspace.Stores:GetDescendants()) do
        if v.Name == Name and v:FindFirstChildOfClass("Humanoid") then
            return v
        end
    end
end
local function buy(im)
    local lteme
    local counter
    repeat
    for i,v in ipairs(workspace.Stores:GetChildren()) do
        if v:FindFirstChild("ShopItems") then
            local ShopItems = v["ShopItems"]
            for o,b in ipairs(ShopItems:GetChildren()) do
                if tostring(b.Name) == im then
                    lteme = b
                    counter = b.Parent.Parent.Counter
                end
            end
        end
    end wait() 
    until lteme ~= nil
    local bypassanticheat = "Ifyouarereadingthisstophackingbrolegitalsokrnlisbadbtw432rewdWdwFe432432rwDWDAVWdawgdGWAYDFGYTUW"
    for e = 1,3 do
        game:GetService("ReplicatedStorage").Interaction.ClientIsDragging:FireServer(lteme,bypassanticheat)
        lteme:PivotTo(counter.CFrame + Vector3.new(0, .4, 0))
        game:GetService("ReplicatedStorage").Interaction.ClientIsDragging:FireServer(lteme,bypassanticheat)
        game:GetService('RunService').Stepped:wait();
    end
    if not TEST then k = listen(im) end
    task.wait(0.15)
    for i = 1,3 do
    local A_1 = 
    {
    	["Character"] = FindCharacter(shoper),
	    ["Name"] = shoper, 
    	["ID"] = cashierIds[shoper], 
    	["Dialog"] = FindCharacter(shoper):FindFirstChild("Dialog")
    }
    local A_2 = "ConfirmPurchase"
    local Event = game:GetService("ReplicatedStorage").NPCDialog.PlayerChatted
    Event:InvokeServer(A_1, A_2)
    task.wait()
    end
end
