Section7:Button("设置传送点",function()
  pcall(function()
    workspace.VirtBasedropCord:Destroy();
    Virt.itemset=nil
  end)
  local VirtBasedropCord=Instance.new("Part")
  VirtBasedropCord.Name="VirtBasedropCord";
  VirtBasedropCord.Anchored=true;
  VirtBasedropCord.Parent=game.Workspace;
  VirtBasedropCord.Shape=Enum.PartType.Ball;
  VirtBasedropCord.Size=Vector3.new(2,2,2)
  VirtBasedropCord.Color=Color3.fromRGB(0, 217, 255);
  VirtBasedropCord.Material=Enum.Material.ForceField;
  VirtBasedropCord.CFrame=lp.Character.HumanoidRootPart.CFrame
  itemset=lp.Character.HumanoidRootPart.CFrame
end)
local tpnum
