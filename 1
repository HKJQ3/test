local Sawmill = Section6:Label("当前剧木机: 无")
Section6:Button("设置指定剧木机", function()
  notify("Virtual","请点击一个剧木机",4)
  SawP=nil
  local ModTree = mouse.Button1Up:Connect(function()
    SawP=nil
    local obj = mouse.Target.Parent
    if obj.Name:find("Sawmill")then
      SawP=sawmill;
      notify("Virtual","剧木机已选择",4)
      Sawmill:SetLabel("当前剧木机: "..tostring(SawP.Name))
     elseif obj.Parent.Name:find("Sawmill")or obj.Parent:FindFirstChild"BlockageAlert"then
      SawP=obj.Parent
      notify("Virtual","剧木机已选择",4)
      Sawmill:SetLabel("当前剧木机: "..tostring(SawP.Name))
    end
  end)
  spawn(function()
    repeat task.wait(0.01) until SawP ~= nil
    ModTree:Disconnect()
    ModTree=nil
  end)
end)
Section6:Toggle("带来树之后自动处理", 'Toggleflag', false, function(state)
  if SawP then
    bringmod=state
   else
    notify("Virtual","请设置指定剧木机")
  end
end)
Section6:Slider('设置等待时间', 'Sliderflag', 1, 1, 1000, false, function(s)
    time = s
end)
Section6:Toggle("自动赚钱[Alpha]", 'Toggleflag', false, function(state)
  if state then
    if bringmod then
      if SelectTrre then
        autofarm = true
        spawn(function()
          while wait(time) do
            if autofarm then
              bringTree(SelectTrre)
            end
          end 
        end)
      else
        notify("Virtual","请选择树")
      end
    else
      notify("Virtual","请开启自动处理")
    end
  else
    autofarm = false
  end
end)

local Sell = Section6:Label("当前出售机: 无")
local SellP
Section6:Button("设置指定出售机", function()
  notify("Virtual","请点击一个出售机",4)
  SellP=nil
  local ModTree = mouse.Button1Up:Connect(function()
    SellP=nil
    local obj = mouse.Target.Parent
    if obj.Name:find("Conveyor") then
        SellP=obj.Parent.Parent
        notify("Virtual","出售机已选择",4)
        Sell:SetLabel("当前出售机: "..tostring(SellP.Name))
    elseif obj.Name:find("Parts") then
        SellP=obj.Parent
        notify("Virtual","出售机已选择",4)
        Sell:SetLabel("当前出售机: "..tostring(SellP.Name))
    elseif obj.Parent.Name:find("Furnace") then
        SellP=obj.Parent
        notify("Virtual","出售机已选择",4)
        Sell:SetLabel("当前出售机: "..tostring(SellP.Name))        
    end  
  end)
  spawn(function()
    repeat task.wait(0.01) until SellP ~= nil
    ModTree:Disconnect()
    ModTree=nil
  end)
end)
local to
Section6:Button("一键卖木板",function()
        for _, Plank in pairs(game.Workspace.PlayerModels:GetChildren()) do
        if Plank.Name == "Plank" and Plank:findFirstChild("Owner") then
            if Plank.Owner.Value == game.Players.LocalPlayer then
                for i, v in pairs(Plank:GetChildren()) do
                    if v.Name == "WoodSection" then
                        spawn(function()
                            for i = 1, 100 do
                                wait()
                                v.CFrame = SellP.Conveyor.Conveyor.CFrame
                            end
                        end)
                    end
                end
                spawn(function()
                    for i = 1, 100 do
                        wait()
                        game.ReplicatedStorage.Interaction.ClientIsDragging:FireServer(Plank,"Ifyouarereadingthisstophackingbrolegitalsokrnlisbadbtw432rewdWdwFe432432rwDWDAVWdawgdGWAYDFGYTUW")
                    end
                end)
            end
        end
    end
end)
