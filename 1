  setclipboard(item)
end)
local tpitems = {}
Section7:Label("格式: 物品代码,物品代码")
Section7:Textbox("多个传送","TextBoxfalg","输入",function(s)
  tpitems=string.split(s,",")
end)
Section7:Toggle("打开多个传送", 'Toggleflag', false, function(state)
  tpmany = state
end)
Section7:Toggle("新物品传送到传送点", 'Toggleflag', false, function(state)
  TEST = state
end)
spawn(function()
    b = workspace.PlayerModels.ChildAdded:Connect(function(child)
        local owner = child:WaitForChild("Owner")
        if owner.Value == lp and TEST and not to then
             game:GetService("ReplicatedStorage").PlaceStructure.ClientPlacedStructure:FireServer(nil,itemset,lp, nil, child, true)         
             local bypassanticheat = "Ifyouarereadingthisstophackingbrolegitalsokrnlisbadbtw432rewdWdwFe432432rwDWDAVWdawgdGWAYDFGYTUW"
             game:GetService("ReplicatedStorage").Interaction.ClientIsDragging:FireServer(child,bypassanticheat)
             child:MoveTo(itemset.p)
             game:GetService("ReplicatedStorage").Interaction.ClientIsDragging:FireServer(child,bypassanticheat)
             game:GetService('RunService').Stepped:wait();
             game:GetService("ReplicatedStorage").PlaceStructure.ClientPlacedStructure:FireServer(nil,itemset,lp, nil, child, true)        
        end
    end)
end)
local concat=1
Section7:Button("传送", function()
  concat=1
  for i,model in pairs(game.Workspace.PlayerModels:GetChildren()) do
    if model:FindFirstChild("Owner") and model.Owner.Value == game.Players[player] then
      if model.Name == item or model:FindFirstChild("PurchasedBoxItemName") and model:FindFirstChild("PurchasedBoxItemName").Value == item or model:FindFirstChild("ItemName") and model:FindFirstChild("ItemName").Value == item then
              models[i]=model
              game:GetService("ReplicatedStorage").PlaceStructure.ClientPlacedStructure:FireServer(nil,itemset,lp, nil, model, true)         
              local bypassanticheat = "Ifyouarereadingthisstophackingbrolegitalsokrnlisbadbtw432rewdWdwFe432432rwDWDAVWdawgdGWAYDFGYTUW"
              game:GetService("ReplicatedStorage").Interaction.ClientIsDragging:FireServer(model,bypassanticheat)
              model:MoveTo(itemset.p)
              game:GetService("ReplicatedStorage").Interaction.ClientIsDragging:FireServer(model,bypassanticheat)
              game:GetService('RunService').Stepped:wait();
              game:GetService("ReplicatedStorage").PlaceStructure.ClientPlacedStructure:FireServer(nil,itemset,lp, nil, model, true)        
              task.wait(Virtual.tpspeed)
              if concat >= tpnum then return end
              concat = concat + 1
      end
    end
  end

end)
Section7:Button("多个传送", function()
  if not tpmany then return end
  for ki = 1,#tpitems do
      for i,model in pairs(game.Workspace.PlayerModels:GetChildren()) do
        if model:FindFirstChild("Owner") and model.Owner.Value == game.Players[player] then
          if model.Name == tpitems[ki] or model:FindFirstChild("PurchasedBoxItemName") and model:FindFirstChild("PurchasedBoxItemName").Value == tpitems[ki] or model:FindFirstChild("ItemName") and model:FindFirstChild("ItemName").Value == tpitems[ki] then
              models[i]=model
              game:GetService("ReplicatedStorage").PlaceStructure.ClientPlacedStructure:FireServer(nil,itemset,lp, nil, model, true)         
              local bypassanticheat = "Ifyouarereadingthisstophackingbrolegitalsokrnlisbadbtw432rewdWdwFe432432rwDWDAVWdawgdGWAYDFGYTUW"
              game:GetService("ReplicatedStorage").Interaction.ClientIsDragging:FireServer(model,bypassanticheat)
              model:MoveTo(itemset.p)
              game:GetService("ReplicatedStorage").Interaction.ClientIsDragging:FireServer(model,bypassanticheat)
              game:GetService('RunService').Stepped:wait();
              game:GetService("ReplicatedStorage").PlaceStructure.ClientPlacedStructure:FireServer(nil,itemset,lp, nil,model, true)         
              task.wait(Virtual.tpspeed)
          end
        end
      end
  end
end)
