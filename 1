local lp=game.Players.LocalPlayer
local mouse = lp:GetMouse()
local ErrorPrompt = getrenv().require(game:GetService("CoreGui").RobloxGui.Modules.ErrorPrompt)
local Virt = {
  cuttreeselect="Generic",
  dxmz="",
  slot=-1,
  zlwjia="",
  zix=1,
  zlz=3,
  bringamount=1,
  bringtree=false,
  itemset=nil,
  tpspeed = 0
  dropdown = {}
}

local function notify(Title,Text,Duration) -- Sends Notification in the bottom right of the screen
    game.StarterGui:SetCore("SendNotification", {
        Title = Title;
        Text = Text;
        Icon = nil;
        Duration = Duration
    })
end
local playeritems = {}
local models = {}
local function getITEMs(player)
    for i, v in pairs(game.Workspace.PlayerModels:GetChildren()) do
        if v:FindFirstChild("Owner") and v.Owner.Value == player then
            if not v:FindFirstChild("BlueprintWoodClass") and not v:FindFirstChild("TreeClass") then          
                if not v.Name:find("Conveyor") then
                    if not table.find(playeritems, tostring(v.Name)) and tostring(v.Name) ~= "Model" or tostring(v.Name) ~= "Box" then
                        table.insert(playeritems, tostring(v.Name))
                    elseif v:FindFirstChild("ItemName") and not table.find(playeritems, v:FindFirstChild("ItemName").Value) then
                        table.insert(playeritems, v:FindFirstChild("ItemName").Value)
                    elseif v:FindFirstChild("PurchasedBoxItemName") and not table.find(playeritems, v:FindFirstChild("PurchasedBoxItemName").Value) then
                        table.insert(playeritems, v:FindFirstChild("PurchasedBoxItemName").Value)
                    end
                    local baseName = tostring(v.Name)
                    if baseName == "Model" or baseName == "Box" then
                    baseName = v.ItemName or v.PurchasedBoxItemName
                    end
                    models[baseName] = v
                end
            end
        end
    end
end
function toolsta(toolname)
    return require(game.ReplicatedStorage.Purchasables.Tools.AllTools[toolname].AxeClass).new()
end

local function table_foreach(table, callback)
    for i=1,#table do
        callback(i, table[i])
    end
end


local function tp(pos)
    if typeof(pos) == "CFrame" then
        lp.Character:SetPrimaryPartCFrame(pos)
    elseif typeof(pos) == "Vector3" then
        lp.Character:MoveTo(pos)
    end
end

local function cutPart (tool,event, treeClass,axestr)
    game:GetService"ReplicatedStorage".Interaction.RemoteProxy:FireServer(event, {
        tool = tool,
        faceVector = Vector3.new(0, 0,-1),
        height = 0.3,
        sectionId = 1,
        hitPoints = axestr,
        cooldown = -14,
        cuttingClass = "Axe"
    })
end
local function dismembered()
    lp.Character.Humanoid:UnequipTools()
    local tool = 0
    for i,v in pairs (lp.Backpack:GetChildren()) do
        if v.Name ~= "BlueprintTool"then
            tool=tool+1
        end
    end
    if tool==0 then
        return
        notify("Virtual","你需要斧头",4)
    end


    for i,v in pairs (lp.Backpack:GetChildren()) do
        if v.Name ~= "BlueprintTool"then
            tool = v
        end
    end
    
    local toolname
    for i ,v in pairs (tool:GetChildren()) do
        if v.Name=="ToolName" then
            toolname=v.Value
            Damagefor=function(toolname)
                return require(game.ReplicatedStorage.Purchasables.Tools.AllTools[toolname].AxeClass).new()
            end
            damage=Damagefor(toolname).Damage
        end
    end

    local LogChopped = false
    
    local sectioon
    dd = game:GetService("Workspace").LogModels.ChildAdded:Connect(function(v)
        if v:FindFirstChild"Owner" and v.Owner.Value == lp then
            if v.Name == "WoodSection" and v:FindFirstChild("ChildIDs") and v:FindFirstChild("ChildIDs"):FindFirstChild("Child") then
                print(v:GetFullName())
                tp(v.CFrame)
            end
        end
    end)
    notify("Virtual", "请你点击一棵树", 4)
    DismemberTreeC = mouse.Button1Up:Connect(function()
        Clicked = mouse.Target
        if Clicked:FindFirstAncestor("LogModels") then
            if Clicked.Parent:FindFirstChild("Owner") and Clicked.Parent.Owner.Value == game.Players.LocalPlayer then
                TreeToJointCut = Clicked.Parent
            end
        end
    end)
    repeat
        task.wait()
    until tostring(TreeToJointCut) ~= "nil"
        for i,v in next, TreeToJointCut:GetChildren() do
            if v.Name == "WoodSection" and v:FindFirstChild("ID") and v:FindFirstChild("ID").Value == 1 then
                repeat
                game:GetService"ReplicatedStorage".Interaction.RemoteProxy:FireServer(v.Parent:FindFirstChild("CutEvent"), {
                    tool = tool,
                    faceVector = Vector3.new(0, 0,-1),
                    height = tonumber(v.Size.Y),
                    sectionId = 1,
                    hitPoints = damage,
                    cooldown = -14,
                    cuttingClass = "Axe"
                })
                task.wait()
                until LogChopped == true
                LogChopped = false
                wait(1) 
            end
        end   
    TreeToJointCut = nil
    dd:Disconnect()
    DismemberTreeC:Disconnect()
end

local treeListener = function(treeClass, callback)
    local childAdded
    a = workspace.LogModels.ChildAdded:Connect(function(child)
        local owner = child:WaitForChild("Owner")
        if owner.Value == lp and child.TreeClass.Value == treeClass then
            a:Disconnect()
            callback(child)
        end
    end)
end
local getBiggestTree = function(treeClass)
    for _,v in next,workspace:children()do
        if tostring(v)=="TreeRegion"then
            for _,g in next,v:children()do
                if g:FindFirstChild("TreeClass")and tostring(g.TreeClass.Value)==treeClass and g:FindFirstChild("Owner")then
                    if g.Owner.Value==nil or tostring(g.Owner.Value)==tostring(lp)then
                        if #g:children() > 5 and g:FindFirstChild("WoodSection")then
                            for h,j in next,g:children() do
                                if j:FindFirstChild("ID")and j.ID.Value==1 and j.Size.Y>.5 then
                                    return j
                                end
                            end
                        end
                    end
                end
            end
        end
    end
    return false
end

if not isfile("NoNNNNVQEBZQGE") then
    local script = [[local a=game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame;for b,c in pairs(game.Workspace.LogModels:GetChildren())do if c.Name:sub(1,6)=="Loose_"and c:findFirstChild("Owner")then if c.Owner.Value==game.Players.LocalPlayer then local d=c;local e="Ifyouarereadingthisstophackingbrolegitalsokrnlisbadbtw432rewdWdwFe432432rwDWDAVWdawgdGWAYDFGYTUW"for f=1,15 do game:GetService("ReplicatedStorage").Interaction.ClientIsDragging:FireServer(d,e)d:PivotTo(a)game:GetService("ReplicatedStorage").Interaction.ClientIsDragging:FireServer(d,e)game:GetService('RunService').Stepped:wait()end end end end]]
    writefile("NoNNNNVQEBZQGE",script)
end
local function NoCooldown()
    lp.Character.Humanoid:UnequipTools()
    for i,v in pairs(lp.Backpack:GetChildren()) do
        if v.Name ~= "BlueprintTool"then
            if v.Name == "Tool" then
                Tool = v
            end
        end
    end
    local Stats = getupvalues(getconnections(Tool.Activated)[1].Function)
    NewStats = Stats[1]
    NewStats.SwingCooldown = 0
    setupvalue(getconnections(Tool.Activated)[1].Function,1,NewStats)
end
local function SetRange(value)
    lp.Character.Humanoid:UnequipTools()
    for i,v in pairs(lp.Backpack:GetChildren()) do
        if v.Name ~= "BlueprintTool"then
            if v.Name == "Tool" then
                Tool = v
            end
        end
    end
    local Stats = getupvalues(getconnections(Tool.Activated)[1].Function)
    NewStats = Stats[1]
    NewStats.Range = value
    setupvalue(getconnections(Tool.Activated)[1].Function,1,NewStats)
end
function modwood()
    local wood
    local Saw
    notify("Virtual", "请点击一颗树", 4)
    local ModTree = mouse.Button1Up:Connect(function()
        local obj = mouse.Target.Parent
        if not obj:FindFirstChild "RootCut" and obj.Parent.Name == "TreeRegion" then
            return notify("错误!", "这棵树还没有砍!", 3)
        end
        if obj:FindFirstChild "Owner" and obj.Owner.Value == lp and obj:FindFirstChild "WoodSection" then
            wood = obj
            notify("Virtual", "已选择树!", 3)
        end
        if obj.Name:find("Sawmill")then
            Saw=sawmill;
            notify("Virtual","剧木机已选择",4)
        elseif obj.Parent.Name:find("Sawmill")or obj.Parent:FindFirstChild"BlockageAlert"then
            Saw=obj.Parent
            notify("Virtual","剧木机已选择",4)
        end
    end)
    repeat task.wait(.01) until wood and Saw ~= nil
    ModTree:Disconnect()
    ModTree = nil
    s = true
    local SawC = Saw.Particles.CFrame + Vector3.new(0.7, 0,0)
    local cu,cw= 0,nil
    for p, I in next, wood:GetChildren() do
        if I.Name == "WoodSection" then
            if I.ID.Value > cu then
                cu = I.ID.Value
                cw=I
            end
        end
    end
    
    for p, I in next, wood:GetChildren() do
        if I.Name == "WoodSection" then
            for p, b in next, I.ChildIDs:GetChildren() do
                if b.Value ==cu then
                    cv = I
                end
            end
        end
    end
    wait()
    local tool= 0
    for i,v in pairs (lp.Backpack:GetChildren()) do
        if v.Name ~= "BlueprintTool" and v:FindFirstChild("ToolName") then
            tool=tool+1
        end
    end
    if tool==0 then
        return
        notify("Virtual","你需要斧头",4)
    end


    for i,v in pairs (lp.Backpack:GetChildren()) do
        if v.Name ~= "BlueprintTool" and v:FindFirstChild("ToolName") then
            if v.Name == "Tool" then
                too = v
            end
        end
    end
    local toolname
    for i ,v in pairs (too:GetChildren()) do
        if v.Name=="ToolName" then
            toolname=v.Value
            Damagefor=function(toolname)
                return require(game.ReplicatedStorage.Purchasables.Tools.AllTools[toolname].AxeClass).new()
            end
            Damage=Damagefor(toolname).Damage
        end
    end
    local function anti()
        if s then
            local a=CFrame.new(999999,999999,999999);for b,c in pairs(game.Workspace.LogModels:GetChildren())do if c.Name:sub(1,6)=="Loose_"and c:findFirstChild("Owner")then if c.Owner.Value==game.Players.LocalPlayer then local d=c;local e="Ifyouarereadingthisstophackingbrolegitalsokrnlisbadbtw432rewdWdwFe432432rwDWDAVWdawgdGWAYDFGYTUW"for f=1,15 do game:GetService("ReplicatedStorage").Interaction.ClientIsDragging:FireServer(d,e)d:PivotTo(a)game:GetService("ReplicatedStorage").Interaction.ClientIsDragging:FireServer(d,e)game:GetService('RunService').Stepped:wait()end end end end
            s = false
        end
    end
    local x = math.random(999,9999)
    local y = math.random(99,999)
    local z = math.random(999,9999)
    game.Workspace["Region_Volcano"].Lava.Lava.CFrame=CFrame.new(x,y,z)
    repeat wait()
    --Fix by 0x15 & Laad
    local ltem = cv
    local bypassanticheat = "Ifyouarereadingthisstophackingbrolegitalsokrnlisbadbtw432rewdWdwFe432432rwDWDAVWdawgdGWAYDFGYTUW"
    for e = 1, 15 do
        game:GetService("ReplicatedStorage").Interaction.ClientIsDragging:FireServer(ltem,bypassanticheat)
        ltem:PivotTo(game.Workspace["Region_Volcano"].Lava.Lava.CFrame)
        game:GetService("ReplicatedStorage").Interaction.ClientIsDragging:FireServer(ltem,bypassanticheat)
        game:GetService('RunService').Stepped:wait();
    end
    anti()

    until cv:FindFirstChild("LavaFire");
    cv:FindFirstChild("LavaFire"):Destroy();
    for i=1,20 do
        cw:PivotTo(cw.CFrame)
        local bypassanticheat = "Ifyouarereadingthisstophackingbrolegitalsokrnlisbadbtw432rewdWdwFe432432rwDWDAVWdawgdGWAYDFGYTUW"
        game:GetService("ReplicatedStorage").Interaction.ClientIsDragging:FireServer(cw,bypassanticheat)
        cw:PivotTo(cw.CFrame)
        cw.CFrame=SawC
        game:GetService("ReplicatedStorage").Interaction.ClientIsDragging:FireServer(cw,bypassanticheat)
        game:GetService('RunService').Stepped:wait();
        cw:WaitForChild("BodyVelocity"):Destroy()
        cw:WaitForChild("BodyAngularVelocity"):Destroy()
        cw:WaitForChild("LavaFire"):Destroy()
        cw.CFrame=SawC
        game:GetService("ReplicatedStorage").Interaction.ClientIsDragging:FireServer(v,bypassanticheat)
        game:GetService('RunService').Stepped:wait();
    end
end
local SawP
local polayer
local function bringTree(treeClass)
    lp.Character.Humanoid:UnequipTools()
    local oldPos=lp.Character.HumanoidRootPart.CFrame;
    local tool
    local tool=0
    for i,v in pairs (lp.Backpack:GetChildren()) do
        if v.Name ~= "BlueprintTool" and v:FindFirstChild("ToolName") then
            tool=tool+1
        end
    end
    if tool==0 then
        return
        notify("Virtual","你需要斧头",4)
    end


    for i,v in pairs (lp.Backpack:GetChildren()) do
        if v.Name ~= "BlueprintTool" and v:FindFirstChild("ToolName") then
            tool = v
        end
    end
    
    local toolname
    for i ,v in pairs (tool:GetChildren()) do
        if v.Name=="ToolName" then
            toolname=v.Value
        end
    end

    local axestr=toolsta(toolname)
    if axestr.SpecialTrees and axestr.SpecialTrees[treeClass] then
        for i, v in next, axestr.SpecialTrees[treeClass] do
            axestr[i] = v
        end
    end
    local tree = getBiggestTree(treeClass)

    if not tree then
        return notify("Virtual","没有找到树",4)
    end




    local treeCut = false
    if treeClass=="LoveCave" and axestr.Damage<10000000 then
        return notify("Virtual","你没有合适的斧头去砍这棵树")
    end
    if treeClass=="Shine" and axestr.Damage<80000000 then
        return notify("Virtual","你没有合适的斧头去砍这棵树")
    end
    if treeClass=="Shine" and axestr.Damage<80000000 then
        return notify("Virtual","你没有合适的斧头去砍这棵树")
    end
    if treeClass=="Magma" and axestr.Damage<10000000 then
        return notify("Virtual","你没有合适的斧头去砍这棵树")
    end
    if treeClass=="Ice" and axestr.Damage<8000000 then
        return notify("Virtual","你没有合适的斧头去砍这棵树")
    end
    if treeClass== "Radioactive" and axestr.Damage<10000000 then
        return notify("Virtual","你没有合适的斧头去砍这棵树")
    end
    if treeClass== "Scale" and axestr.Damage<40000000 then
        return notify("Virtual","你没有合适的斧头去砍这棵树")
    end

    workspace.Camera.CameraSubject = tree
    treeListener(treeClass, function(tree)
        tree.PrimaryPart = tree:FindFirstChild("WoodSection")
        treeCut = true
        wait()
        workspace.Camera.CameraSubject = lp.Character
        if bringToPlayer and polayer ~= lp.Name then
            if bringmod then return end
            local oldPosition = game.Players[polayer].Character.HumanoidRootPart.CFrame
            local ltem = tree
            local bypassanticheat = "Ifyouarereadingthisstophackingbrolegitalsokrnlisbadbtw432rewdWdwFe432432rwDWDAVWdawgdGWAYDFGYTUW"
            for e = 1, 15 do
                game:GetService("ReplicatedStorage").Interaction.ClientIsDragging:FireServer(ltem,bypassanticheat)
                ltem:PivotTo(oldPosition)
                game:GetService("ReplicatedStorage").Interaction.ClientIsDragging:FireServer(ltem,bypassanticheat)
                game:GetService('RunService').Stepped:wait();
            end
        end
        if bringmod then
            local wood = tree
            local Saw = SawP
            s = true
            local SawC = Saw.Particles.CFrame + Vector3.new(0.7, 0,0)
            local cu,cw= 0,nil
            for p, I in next, wood:GetChildren() do
                if I.Name == "WoodSection" then
                    if I.ID.Value > cu then
                        cu = I.ID.Value
                        cw=I
                    end
                end
            end
            
            for p, I in next, wood:GetChildren() do
                if I.Name == "WoodSection" then
                    for p, b in next, I.ChildIDs:GetChildren() do
                        if b.Value ==cu then
                            cv = I
                        end
                    end
                end
            end
            wait()
            local tool= 0
            for i,v in pairs (lp.Backpack:GetChildren()) do
                if v.Name ~= "BlueprintTool" and v:FindFirstChild("ToolName") then
                    tool=tool+1
                end
            end
            if tool==0 then
                return notify("Virtual","你需要斧头",4)
            end

            for i,v in pairs (lp.Backpack:GetChildren()) do
                if v.Name ~= "BlueprintTool" and v:FindFirstChild("ToolName") then
                    too = v
                end
            end
            local toolname
            for i ,v in pairs (too:GetChildren()) do
                if v.Name=="ToolName" then
                    toolname=v.Value
                    Damagefor=function(toolname)
                        return require(game.ReplicatedStorage.Purchasables.Tools.AllTools[toolname].AxeClass).new()
                    end
                    Damage=Damagefor(toolname).Damage
                end
            end
            local function anti()
                if s then
                    local a=CFrame.new(999999,999999,999999);for b,c in pairs(game.Workspace.LogModels:GetChildren())do if c.Name:sub(1,6)=="Loose_"and c:findFirstChild("Owner")then if c.Owner.Value==game.Players.LocalPlayer then local d=c;local e="Ifyouarereadingthisstophackingbrolegitalsokrnlisbadbtw432rewdWdwFe432432rwDWDAVWdawgdGWAYDFGYTUW"for f=1,15 do game:GetService("ReplicatedStorage").Interaction.ClientIsDragging:FireServer(d,e)d:PivotTo(a)game:GetService("ReplicatedStorage").Interaction.ClientIsDragging:FireServer(d,e)game:GetService('RunService').Stepped:wait()end end end end
                    s = false
                end
            end
            local x = math.random(999,9999)
            local y = math.random(99,999)
            local z = math.random(999,9999)
            game.Workspace["Region_Volcano"].Lava.Lava.CFrame=CFrame.new(x,y,z)
            repeat wait()
            --Fix by 0x15 & Laad
            local ltem = cv
            local bypassanticheat = "Ifyouarereadingthisstophackingbrolegitalsokrnlisbadbtw432rewdWdwFe432432rwDWDAVWdawgdGWAYDFGYTUW"
            for e = 1, 15 do
                game:GetService("ReplicatedStorage").Interaction.ClientIsDragging:FireServer(ltem,bypassanticheat)
                ltem:PivotTo(game.Workspace["Region_Volcano"].Lava.Lava.CFrame)
                game:GetService("ReplicatedStorage").Interaction.ClientIsDragging:FireServer(ltem,bypassanticheat)
                game:GetService('RunService').Stepped:wait();
            end
            anti()
            until cv:FindFirstChild("LavaFire");
            cv:FindFirstChild("LavaFire"):Destroy();
            for i=1,20 do
                cw:PivotTo(cw.CFrame)
                local bypassanticheat = "Ifyouarereadingthisstophackingbrolegitalsokrnlisbadbtw432rewdWdwFe432432rwDWDAVWdawgdGWAYDFGYTUW"
                game:GetService("ReplicatedStorage").Interaction.ClientIsDragging:FireServer(cw,bypassanticheat)
                cw:PivotTo(cw.CFrame)
                cw.CFrame=SawC
                game:GetService("ReplicatedStorage").Interaction.ClientIsDragging:FireServer(cw,bypassanticheat)
                game:GetService('RunService').Stepped:wait();
                cw:WaitForChild("BodyVelocity"):Destroy()
                cw:WaitForChild("BodyAngularVelocity"):Destroy()
                cw:WaitForChild("LavaFire"):Destroy()
                cw.CFrame=SawC
                game:GetService("ReplicatedStorage").Interaction.ClientIsDragging:FireServer(v,bypassanticheat)
                game:GetService('RunService').Stepped:wait();
            end
        else
            if polayer ~= lp.Name then return end
            local oldPosition = lp.Character.HumanoidRootPart.CFrame
            local ltem = tree
            local bypassanticheat = "Ifyouarereadingthisstophackingbrolegitalsokrnlisbadbtw432rewdWdwFe432432rwDWDAVWdawgdGWAYDFGYTUW"
            for e = 1, 15 do
                game:GetService("ReplicatedStorage").Interaction.ClientIsDragging:FireServer(ltem,bypassanticheat)
                ltem:PivotTo(oldPosition)
                game:GetService("ReplicatedStorage").Interaction.ClientIsDragging:FireServer(ltem,bypassanticheat)
                game:GetService('RunService').Stepped:wait();
            end
        end
    end)  
    task.wait()
    repeat
    cutPart(tool,tree.Parent.CutEvent, treeClass,axestr.Damage)
    wait()
    until treeCut
end
