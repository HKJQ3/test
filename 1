function shuaxinlb(zji)
    Virt.dropdown = {}
    for _, plr in ipairs(game.Players:GetChildren()) do
        if zji or plr ~= lp then
            table.insert(Virt.dropdown, plr.Name)
        end
    end
end
shuaxinlb(true)


